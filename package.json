{
  "name": "aws-cdk-devops-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:be": "AWS_PROFILE=$AWS_PROFILE sst dev --stage $SST_STAGE",
    "dev:fe": "npm run dev -w @app/frontend",
    "build": "AWS_PROFILE=$AWS_PROFILE sst build --stage $SST_STAGE",
    "deploy": "AWS_PROFILE=$AWS_PROFILE sst deploy --stage $SST_STAGE",
    "remove": "AWS_PROFILE=$AWS_PROFILE sst remove --stage $SST_STAGE",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "prepare": "node scripts/prepare.js",
    "preinstall": "npm i -g husky@8.0.3",
    "postinstall": "npm run postinstall --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint-ci": "run-p lint:ci lint:sass lint:ls adviser:ci",
    "lint-dev": "run-p lint:dev lint:sass lint:ls adviser:dev",
    "lint-staged": "npm run lint-staged --workspaces --if-present",
    "lint:ls": "npx @ls-lint/ls-lint",
    "lint:ci": "npm run lint:ci --workspaces --if-present",
    "lint:dev": "npm run lint:dev --workspaces --if-present",
    "lint:sass": "stylelint \"./packages/**/*.scss\""
  },
